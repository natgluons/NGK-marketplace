group>\r\n        <button class=\"prev\" @click=\"prevImage\">&#10094;</button>\r\n        <button class=\"next\" @click=\"nextImage\">&#10095;</button>\r\n      </div>\r\n      <div class=\"dots-container\">\r\n        <div \r\n          v-for=\"(dot, index) in breedData.images\" \r\n          :key=\"index\"\r\n          :class=\"['dot', { 'dot-active': currentIndex === index }]\"\r\n          @click=\"currentIndex = index\"\r\n        ></div>\r\n      </div>\r\n    </div>\r\n    <div class=\"description\">\r\n      <div style=\"text-align: center;\"><strong>{{ breedName }}</strong></div>\r\n      <br />\r\n      Available colors:\r\n    </div>\r\n    <div class=\"available-colors\">\r\n      <div v-for=\"color in formattedColors\" :key=\"color.name\" :class=\"color.class\">\r\n        <div class=\"overlap-group-2\">\r\n          <div class=\"text-wrapper-2\">{{ color.name }}</div>\r\n          <div :class=\"color.ellipseClass\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"description\">\r\n      {{ breedData.description }}<br /><br />\r\n      Price range: {{ breedData.price }}<br />\r\n    </div>\r\n    <div class=\"advertise\">\r\n      <strong>Interested to take home our puppy?</strong>\r\n      <br />Book your visit now!<br /><br />\r\n      Contact us for pricing, availability, and visitation schedule.<br /><br />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport breedsData from '@/assets/breedsData.js';\r\n\r\nexport default {\r\n  name: 'BreedPage',\r\n  data() {\r\n    return {\r\n      breedName: 'Toy Poodle', // Replace with dynamic breed name if needed\r\n      currentIndex: 0,\r\n      breedData: breedsData['Toy Poodle'] // Set default breed data\r\n    };\r\n  },\r\n  computed: {\r\n    formattedColors() {\r\n      return [\r\n        { name: 'White', class: 'white', ellipseClass: 'ellipse' },\r\n        { name: 'Silver', class: 'silver', ellipseClass: 'ellipse-2' },\r\n        { name: 'Black', class: 'black', ellipseClass: 'ellipse-3' },\r\n        { name: 'Apricot', class: 'apricot', ellipseClass: 'ellipse-4' },\r\n        { name: 'Brown', class: 'brown', ellipseClass: 'ellipse-5' },\r\n        { name: 'Red', class: 'red', ellipseClass: 'ellipse-6' }\r\n      ];\r\n    }\r\n  },\r\n  methods: {\r\n    prevImage() {\r\n      this.currentIndex = (this.currentIndex + this.breedData.images.length - 1) % this.breedData.images.length;\r\n    },\r\n    nextImage() {\r\n      this.currentIndex = (this.currentIndex + 1) % this.breedData.images.length;\r\n    }\r\n  },\r\n  created() {\r\n    setInterval(() => {\r\n      this.nextImage();\r\n    }, 5000); // Auto-change image every 5 seconds\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.breed-page {\r\n  background-color: #ffffff;\r\n  overflow-y: visible;\r\n  overflow-x: hidden;\r\n  width: 100%;\r\n  max-width: 380px;\r\n  height: calc(100vh - 90px);\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.carousel-container {\r\n  position: relative;\r\n}\r\n\r\n.carousel-images {\r\n  display: flex;\r\n  overflow: hidden;\r\n}\r\n\r\n.carousel-image {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.dots-container {\r\n  text-align: center;\r\n  position: absolute;\r\n  bottom: 10px;\r\n  width: 100%;\r\n}\r\n\r\n.dot {\r\n  display: inline-block;\r\n  width: 10px;\r\n  height: 10px;\r\n  background-color: #bbb;\r\n  border-radius: 50%;\r\n  margin: 0 2px;\r\n  cursor: pointer;\r\n}\r\n\r\n.dot-active {\r\n  background-color: #717171;\r\n}\r\n\r\nbutton {\r\n  position: absolute;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  color: white;\r\n  border: none;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.prev {\r\n  left: 10px;\r\n}\r\n\r\n.next {\r\n  right: 10px;\r\n}\r\n\r\n.description {\r\n  font-family: \"Inter-Regular\", Helvetica;\r\n  color: #9aa0a6;\r\n  font-size: 16px;\r\n  letter-spacing: 0.32px;\r\n  line-height: 19.2px;\r\n  margin: 15px;\r\n}\r\n\r\n.advertise {\r\n  font-family: \"Inter-Regular\", Helvetica;\r\n  color: #222222;\r\n  font-size: 18px;\r\n  letter-spacing: 0.3px;\r\n  line-height: 20.1px;\r\n  margin: 20px;\r\n}\r\n\r\n.available-colors {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 5px;\r\n  margin-top: 10px;\r\n  margin-left: 15px;\r\n  margin-right: 0;\r\n}\r\n\r\n.overlap-group-2 {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  background-color: #ffffff;\r\n  border-radius: 10px;\r\n  border: 1px solid #9aa0a6;\r\n  padding: 4px;\r\n  width: 100px;\r\n  height: 30px;\r\n}\r\n\r\n.text-wrapper-2 {\r\n  margin-left: 10px;\r\n  font-family: \"Inter-Regular\", Helvetica;\r\n  color: #9aa0a6;\r\n  font-size: 16px;\r\n}\r\n\r\n.carousel {\r\n  display: flex;\r\n  width: 100%;\r\n  gap: 15px;\r\n  overflow-x: scroll;\r\n  padding: 10px 0;\r\n  scroll-snap-type: x mandatory;\r\n}\r\n\r\n.carousel::-webkit-scrollbar {\r\n  height: 6px;\r\n}\r\n\r\n.carousel::-webkit-scrollbar-track {\r\n  background: #f1f1f1;\r\n}\r\n\r\n.carousel::-webkit-scrollbar-thumb {\r\n  background: #888;\r\n  border-radius: 3px;\r\n}\r\n\r\n.carousel::-webkit-scrollbar-thumb:hover {\r\n  background: #555;\r\n}\r\n\r\n.ellipse, .ellipse-2, .ellipse-3, .ellipse-4, .ellipse-5, .ellipse-6 {\r\n  width: 19px;\r\n  height: 19px;\r\n  border-radius: 50%;\r\n  border: 1px solid #9aa0a6;\r\n  margin-left: 10px;\r\n  margin-right: 8px;\r\n}\r\n\r\n.ellipse { background-color: #fffcfc; }\r\n.ellipse-2 { background-color: #787878; }\r\n.ellipse-3 { background-color: #292525; }\r\n.ellipse-4 { background-color: #cc956d; }\r\n.ellipse-5 { background-color: #97623c; }\r\n.ellipse-6 { background-color: #d03622; }\r\n</style>\r\n\r\n\r\n<script>\r\nimport breedsData from '@/assets/breedsData.js';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      breedName: 'Toy Poodle', // Replace with the desired breed name or dynamic value\r\n      currentIndex: 0,\r\n      breedData: breedsData['Toy Poodle'] // Set the default breed data or retrieve dynamically\r\n    };\r\n  },\r\n  watch: {\r\n    breedName(newBreedName) {\r\n      this.breedData = breedsData[newBreedName];\r\n      this.currentIndex = 0; // Reset to the first image when the breed changes\r\n    }\r\n  },\r\n  methods: {\r\n    prevImage() {\r\n      this.currentIndex = (this.currentIndex + this.breedData.images.length - 1) % this.breedData.images.length;\r\n    },\r\n    nextImage() {\r\n      this.currentIndex = (this.currentIndex + 1) % this.breedData.images.length;\r\n    }\r\n  },\r\n  // created() {\r\n  //   setInterval(() => {\r\n  //     this.nextImage();\r\n  //   }, 5000); // Auto-change image every 5 seconds\r\n  // }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.carousel-container {\r\n  position: relative;\r\n  width: 100%;\r\n  overflow: hidden;\r\n}\r\n\r\n.carousel-images {\r\n  display: flex;\r\n  transition: opacity 0.5s;\r\n}\r\n\r\n.carousel-image {\r\n  width: 100%;\r\n  height: auto;\r\n  margin: 0 auto; \r\n}\r\n\r\n.dots-container {\r\n  text-align: center;\r\n  position: absolute;\r\n  bottom: 10px;\r\n  width: 100%;\r\n}\r\n\r\n.dot {\r\n  display: inline-block;\r\n  width: 10px;\r\n  height: 10px;\r\n  background-color: #bbb;\r\n  border-radius: 50%;\r\n  margin: 0 2px;\r\n  cursor: pointer;\r\n}\r\n\r\n.dot-active {\r\n  background-color: #717171;\r\n}\r\n\r\nbutton {\r\n  position: absolute;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  color: white;\r\n  border: none;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.prev {\r\n  left: 10px;\r\n}\r\n\r\n.next {\r\n  right: 10px;\r\n}\r\n</style>\r\n"]}]}